    <!--================login_part Area =================-->
    <section class="login_part padding_top">
        <div class="container">
            <div class="container">
                <div class="row justify-content-md-center" style="margin-top: 13%;">
                    <div class="col-md-4">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="text-center">
                                    <h4><i class="fas fa-lock" style="font-size:2em;"></i></h4>
                                    <h3 class="text-center">FORGOT  PASSWORD..?</h3>
                                    <p style="font-weight: 500;">Enter your e-mail address and we'll send you a link to reset your password.</p><br>
                                    <div class="panel-body">
            
                                      
                                        <form method="post" action="/forgotPassword">
                                            <div class="form-group">
                                                <label for="email">Email Address</label>
                                                <div class="input-group mb-3">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="basic-addon1"><i
                                                                class="fas fa-envelope"></i></span>
                                                    </div>
                                                    <input class="form-control" id="email" name="email"
                                                           placeholder="Email Address" type="text"/>
                                                </div>
                                                
                                            </div>
                                            <hr>
                                            <% if (typeof message !== 'undefined') { %>
                                                <h6 style="color: green; font-weight: 500; text-align: center;"><%= message %></h6>
                                            <% } %>
                                            <div class="form-group">
                                                <button class="btn_3" style="border-radius: 10px;" type="submit" value="send Reset Link">Reset Password</button>
                                            </div>
                                        </form>
                                        
            
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12" style="font-weight: 600;">
                                New user? <a href="signup">Register</a>
                            </div><br><br>
                            <div class="col-md-12" style="font-weight: 600;">
                                Already registered? <a href="login">Login</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
