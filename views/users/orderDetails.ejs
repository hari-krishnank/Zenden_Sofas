    <!-- order History start -->
    <section>
        <div class="container">
            <div class="shopping-cart-wrapper pt-10 pb-20 flex items-start gap-6">
                <!-- shopping cart start -->
                <div class="shopping-cart w-full">
                    <div class="px-6 py-6 overflow-x-auto">
                        <!-- Add pagination links above or below the table -->
                        <% if (orders.length === 0) { %>
                            <div class="text-center py-6">
                                <h2 style="font-size: 20px; font-weight: 600;">You have no orders</h2><br>
                                
                                <a href="/shop" class="btn_3"style="font-size: 14px; border-radius: 5px;">START SHOPPING</a>
    
                            </div>
                        <% } else { %>

                        <table class="min-w-[1270px] w-full leading-normal">
                            <thead>
                                <tr>
                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[160px]">
                                        Order
                                    </th>
                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[200px]">
                                        Date
                                    </th>
                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[160px]">
                                        Product Name
                                    </th>
                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[160px]">
                                        Total Product
                                    </th>

                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[140px]">
                                        Toral price
                                    </th>
                                    <th
                                        class="pb-6 border-b border-[#E1E3E6] text-left text-xs font-semibold text-[#272343] uppercase tracking-wider w-[120px]">
                                        Status
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% orders.forEach(order=> { %>
                                    <% order.items.forEach(item=> { %>

                                        <tr>
                                           
                                            <td class="py-6 text-sm">
                                                <a href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <%= order._id%>
                                                </a>
                                            </td>

                                            <td class="py-6 text-sm"><a
                                                    href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <%= order.date %>
                                                </a></td>
                                                <td class="py-6 text-sm"><a href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <% if (item.product_id) { %>
                                                        <%= item.product_id.name %>
                                                    <% } else { %>
                                                        No product available
                                                    <% } %>
                                                </a></td>
                                            <td class="py-6 text-sm">


                                                <a href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <%= item.quantity %>
                                                </a><br>

                                            </td class="py-6 text-sm">

                                            <td class="py-6 text-sm"><a
                                                    href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <%= item.total_price %>
                                                </a></td>
                                            <td class="py-6 text-sm">

                                                <a href="/orderSingle?orderId=<%= order._id %>&itemId=<%= item._id %>">
                                                    <button
                                                        class="btn-success2 px-3 py-2 text-[#01AD5A] text-[14px] leading-[120%] font-display"
                                                        style="font-size: 11px; border-radius: 5px; color: green;"
                                                        data-product-id="">
                                                        <%=order.status%>
                                                    </button>
                                                </a>

                                            </td>
                                        </tr>
                                        <% }) %>
                                            <% }); %>
                            </tbody>
                        </table><% } %><br>    
                        <div class="pagination" style="display: flex; justify-content: center;">
                            <% if (currentPage > 1) { %>
                                <a class="btn btn-success2" href="?padisplay: flex- justify-content: center; 1 %>">Previous</a>
                            <% } %>
                            
                            <% for (let i = 1; i <= totalPages; i++) { %>
                                <% if (i === currentPage) { %>
                                    <span class="btn btn-success2"><%= i %></span>
                                <% } else { %>
                                    <a  class="btn btn-success2" href="?page=<%= i %>"><%= i %></a>
                                <% } %>
                            <% } %>
                            
                            <% if (currentPage < totalPages) { %>
                                <a class="btn btn-success2" href="?page=<%= currentPage + 1 %>">Next</a>
                                
                            <% } %>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </section>
    <!-- order History end -->


