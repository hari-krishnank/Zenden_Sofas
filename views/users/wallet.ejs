    <section>
        <div class="container px-3 md:px-5 xl:px-0 xl:pt-10 xl:pb-20 py-8">
            
            <div class="shopping-cart">
                <div class="px-6 py-6 overflow-x-auto">

                    <div class="d-flex flex-column align-items-center mb-5">
                        <img src="/img/wallet.jpg" alt="img" width="200px">
                        <p class="mt-2" style="font-size: 28px; font-weight: bold; font-family: 'Times New Roman', Times, serif;">Balance:â‚¹ <%= user.wallet %></p>
                    </div>
                
                    <h3 style="text-align: center; font-size: 25px; color: #ff3368; font-family: 'Times New Roman', Times, serif;">Transaction History</h3><br>
                    
                    <table style="background-color: #ededed;" class="table table-bordered text-center">
                        <tbody>
                            <% if (user.wallet_history.length > 0) { %>
                                <% user.wallet_history.forEach(function(wallet) { %>
                                    <tr>
                                        <td class="ms-2">
                                            <% 
                                                var isoDate = wallet.date.toISOString().substr(0, 10);
                                                var parts = isoDate.split('-');
                                                var formattedDate = parts[1] + '/' + parts[2] + '/' + parts[0];
                                            %>
                                            <%= formattedDate %>
                                        </td>
                                        
                                        <% if (wallet.amount > 0) { %>
                                            <td class="text-success">â‚¹<%= wallet.amount.toFixed(2) %></td>
                                        <% } else { %>
                                            <td class="text-danger">â‚¹<%= wallet.amount.toFixed(2) %></td>
                                        <% } %>
                                        <td>
                                            <%= wallet.description %>
                                        </td>
                                    </tr>
                                <% }); %>
                            <% } else { %>
                                <tr>
                                    <td style="font-weight: bold; font-family: 'Times New Roman', Times, serif; font-size: large;">No Transactions Found</td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                    
                </div>
            </div>
           
        </div>
