                </svg>
                <span class="text-[14px] font-medium leading-[110%] font-display text-gray-black inline-block">Order confirmation</span>
            </div>

            <h2 class="pt-[13.5px] xl:text-2xl text-[18px] font-semibold text-gray-black font-display">Order confirmation</h2>
        </div>
    </section>
  <!-- breadcrumb start-->

  <!--================ confirmation part start =================-->
  <section class="confirmation_part padding_top">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="confirmation_tittle">
            <span style="font-size: medium; font-weight: bold;">THANK YOU. YOUR ORDER HAS BEEN RECIEVED.</span>
          </div>
        </div>
        <div class="table-responsive">
          <% if (order && order.items) { %>
            <table class="table">

              <thead>
                <tr>
                  <th scope="product-image" style="font-weight: bold;">PRODUCT</th>
                  <th scope="product-name" style="font-weight: bold;">NAME</th>
                  <th scope="product-price" style="font-weight: bold;">PRICE</th>
                  <th scope="product-quantity" style="font-weight: bold;">QUANTITY</th>
                  <th scope="product-total" style="font-weight: bold;">TOTAL</th>
                </tr>
              </thead>
              <tbody>
                <% order.items.forEach((product)=> { %>
                  <tr class="table-body-row">
                    <td class="product-image"><img src="/resizedImages/<%= product.product_id.image[0] %>" alt="img"
                        style="width: 100px; height: auto;">
                    </td>
                    <td class="product-name">
                      <%= product.product_id.name %>
                    </td>
                    <td class="product-price">₹<%= product.price %>
                    </td>
                    <td class="quantity">
                      <div class="input-group mb-3" style="width: 80px; position: relative; ">
                        <input style="text-align: center;" type="text" name="quantity"
                          class="quantity form-control input-number" min="1" max="100" value="<%= product.quantity %>"
                          readonly>
                      </div>
                    </td>
                    <td class="product-total">₹<%= product.total_price %>
                    </td>
                  </tr>
              </tbody>
              <% } )%>
            </table>
        </div>
        <% } %>
          <div class="col-lg-6 col-lx-4">
            <div class="single_confirmation_details">
              <h4>ORDER SUMMARY</h4>
              <ul>
                <li>
                  <p>Order Id</p><span>: <%= order && order._id %></span>
                </li><br>
                <li>
                  <p>Delivery Address</p><span>: <%= order && order.delivery_address %></span>
                </li><br>
                <li>
                  <p>payment Method</p><span>: <%= order && order.payment %></span>
                </li><br>
                <li>
                  <p>Date</p><span>: <%= order && order.date %> </span>
                </li><br>
                <li>
                  <p>Total Amount of Order</p><span>: <%= order && order.total_amount %> </span>
                </li><br>
                <li>
                  <p>Expected Delivery</p><span>: <%= order && order.expected_delivery%> </span>
                </li><br>
              </ul>
            </div>
          </div>


        </div><br>
        <a class="btn_1 genric-btn primary-border radius" href="/userOrders" style="font-weight: bold;">VIEW ORDERS</a>

    </div>
  </section>
