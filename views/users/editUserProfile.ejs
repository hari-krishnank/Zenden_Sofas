    <script>
        function validateForm() {
            // Get form inputs
            var name = document.forms["editProfileForm"]["name"].value;
            var mobile = document.forms["editProfileForm"]["mobile"].value;
    
            // Simple validation for username (not empty)
            if (name == "") {
                alert("Username must be filled out");
                return false;
            }
    
            // Simple validation for mobile (not empty and numeric)
            if (mobile == "") {
                alert("Mobile must be filled out");
                return false;
            } else if (isNaN(mobile)) {
                alert("Mobile must be a number");
                return false;
            }
    
            // If all validations pass, you can submit the form
            return true;
        }
    </script>
    
